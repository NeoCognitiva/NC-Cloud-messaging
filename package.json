{
	"name": "smartbots_messaging",
	"version": "1.0.0",
	"description": "Start building your next Node.js Express app on IBM Cloud.",
	"private": true,
	"main": "app.js",
	"engineStrict": true,
	"engines": {
		"node": "12.14.1",
		"npm": "6.13.4"
	},
	"config": {
		"entrypoint": "app.js"
	},
	"scripts": {
		"test": "mocha test --exit -t 40000",
		"test-report": "cross-env NODE_ENV=test nyc mocha --exit --timeout=40000",
		"start": "node $npm_package_config_entrypoint",
		"debug": "node --inspect=0.0.0.0:9229 $npm_package_config_entrypoint"
	},
	"nodemonConfig": {
		"env": {
			"NODE_HEAPDUMP_OPTIONS": "nosignal"
		}
	},
	"dependencies": {
		"amqp-connection-manager": "^3.2.0",
		"amqplib": "^0.5.6",
		"bent": "^7.3.9",
		"body-parser": "^1.18.3",
		"compression": "^1.7.4",
		"consolidate": "^0.15.1",
		"cookie-parser": "^1.4.5",
		"cookie-session": "^1.4.0",
		"debug": "^4.1.1",
		"dotenv": "^8.2.0",
		"express": "^4.16.4",
		"helmet": "^3.23.3",
		"http-errors": "^1.8.0",
		"ibm-cloud-env": "^0.2.7",
		"moment": "^2.27.0",
		"moment-timezone": "^0.5.31",
		"mongodb": "^3.5.9",
		"morgan": "^1.10.0",
		"node-cron": "^2.0.3",
		"swagger-jsdoc": "^3.7.0",
		"swagger-ui-express": "^4.1.4",
		"winston": "^2.4.5",
		"winston-daily-rotate-file": "^3.10.0",
		"ws": "^7.3.1"
	},
	"devDependencies": {
		"ansi-colors": "^4.1.1",
		"chai": "^4.2.0",
		"chai-as-promised": "^7.1.1",
		"cross-env": "^7.0.2",
		"eslint": "^6.8.0",
		"faker": "^4.1.0",
		"fancy-log": "^1.3.3",
		"fs-extra": "^8.1.0",
		"gulp": "^4.0.2",
		"gulp-eslint": "^6.0.0",
		"gulp-jsdoc3": "^2.0.0",
		"gulp-plumber": "^1.2.1",
		"mocha": "^7.2.0",
		"nodemon": "^2.0.4",
		"nyc": "^15.1.0",
		"sinon": "^8.1.1",
		"sinon-chai": "^3.5.0",
		"supertest": "^4.0.2"
